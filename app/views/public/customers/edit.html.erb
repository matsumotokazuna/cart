<h2>会員編集</h2>
<%= form_for(@customer) do |f| %>
<div>
	<%= f.label :id %>
	<%= f.text_field :id %>
</div>
<div>
	<%= f.label :email %>
	<%= f.email_field :email %>
</div>
<div>
	<%= f.label :姓 %>
	<%= f.text_field :last_name %>
</div>
<div>
	<%= f.label :名 %>
	<%= f.text_field :first_name %>
</div>
<div>
	<%= f.label :郵便番号 %>
	<%= f.text_field :postcode %>
</div>
<div>
	<%= f.label :住所 %>
	<%= f.text_field :address %>
</div>
<div>
	<%= f.submit '編集する'%>
</div>

<% end %>